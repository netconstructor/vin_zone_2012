<?xml version="1.0" encoding="utf-8"?>
<xsd:schema xmlns="http://schemas.monster.com/Monster" elementFormDefault="qualified" targetNamespace="http://schemas.monster.com/Monster" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <xsd:include schemaLocation="Enumerations.xsd" />
  <xsd:complexType name="ContactType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StructuredName" type="StructuredNameType" />
      <xsd:element minOccurs="0" ref="CompanyName" />
      <xsd:element minOccurs="0" name="Address" type="AddressType" />
      <xsd:element minOccurs="0" name="Phones">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" maxOccurs="unbounded" name="Phone" nillable="true" type="PhoneType">
              <xsd:annotation>
                <xsd:documentation>
                  <datatype xmlns="http://schemas.monster.com/Monster">In Job the minimum length is 8, and in Resume there is no minimum length required. The "unknownPrimary" and "unknownSecondary" phone types are supported outbound only and are used for phones captured without a phone type </datatype>
                  <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" name="E-mail" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WebSite" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AdditionalEmailAddresses" type="AdditionalEmailAddressesType" />
      <xsd:element minOccurs="0" name="IMHandles" type="IMHandlesType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CompanyContactType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="ContactType">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="Title" nillable="true" type="xsd:string">
            <xsd:annotation>
              <xsd:documentation>
                <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
                <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="HideableContactType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Indicates if the specified attribute is hidden on the JobPosting that JobSeekers can view.</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent mixed="false">
      <xsd:extension base="ContactType">
        <xsd:attribute default="false" name="hideAll" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideAddress" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideStreetAddress" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideCity" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideState" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hidePostalCode" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideCountry" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideContactInfoField" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideCompanyName" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideEmailAddress" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideFax" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideName" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hidePhone" type="xsd:boolean" use="optional" />
        <xsd:attribute default="false" name="hideStreetAddress2" type="xsd:boolean" use="optional" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="StructuredNameType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="GivenName" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The first name of the seeker or recruiter</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required for JobSeekers</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MiddleName" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The middle name of the seeker or recruiter</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FamilyName" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The last name of the seeker or recruiter</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required for JobSeekers</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NamePrefix" nillable="true" type="NamePrefixEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The surname of the seeker</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="NameSuffix" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The name suffix of the seeker</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">12</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AddressType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="StreetAddress" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the street address, address line 1 for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StreetAddress2" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Represents the street address, address line 2 for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="City" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the City/Town for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="State" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the State/Province for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">64</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CountryCode" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the Country for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PostalCode" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the Postal/ZIP Code for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">20</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PhysicalAddressType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="StreetAddress" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the street address, address line 1 for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="StreetAddress2" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Represents the street address, address line 2 for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="City" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the City/Town for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="State" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the State/Province for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">64</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CountryCode" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the Country for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PostalCode" nillable="true" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> Represents the Postal/ZIP Code for specifying a location for a job posting</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">20</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="OtherAddressType">
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="unspecified" />
            <xsd:enumeration value="multipleLocations" />
            <xsd:enumeration value="telecommute" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="LocationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="City" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">For a job posting it represents the City used to determine the advertisement job search area</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="State" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">For a job posting it represents the State abbreviation used to determine the advertisement job search area</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CountryCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">For a job posting it represents the Country abbreviation used to determine the advertisement job search area</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PostalCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">For a job posting it represents the PostalCode used to determine the advertisement job search area</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Continent" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">20</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="locationId" type="xsd:integer" use="optional" />
    <xsd:attribute name="parentLocationId" type="xsd:integer" use="optional" />
  </xsd:complexType>
  <xsd:complexType name="PhoneType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Adds support for the "contact type preference" that is supported by the Monster database. In addition to specifying a "phone type" (such as 'home' or 'work'), this allows the user to specify which number is 'primary' and which is 'secondary'.
Note: ?priority? is only used for the phone numbers for seekers ? it is not used on the phone numbers for jobs. Additionally, for jobs, only ?fax? and ?contact? are possible values. Phone types of unknownPrimary and unknownSecondary are for legacy outbound use only and will not be accepted inbound.
</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="phoneType" use="required">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="fax" />
              <xsd:enumeration value="work" />
              <xsd:enumeration value="home" />
              <xsd:enumeration value="pager" />
              <xsd:enumeration value="mobile" />
              <xsd:enumeration value="unknownPrimary" />
              <xsd:enumeration value="unknownSecondary" />
              <xsd:enumeration value="other" />
              <xsd:enumeration value="contact" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
        <xsd:attribute name="priority">
          <xsd:simpleType>
            <xsd:restriction base="xsd:string">
              <xsd:enumeration value="primary" />
              <xsd:enumeration value="secondary" />
            </xsd:restriction>
          </xsd:simpleType>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="AdditionalEmailAddressesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="AdditionalEmailAddress" type="AdditionalEmailAddressType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AdditionalEmailAddressType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="EmailAddressType" type="EmailAddressTypeEnumStr" />
      <xsd:element minOccurs="0" name="E-mail" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The email address</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="ProfessionalLicensesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="ProfessionalLicense" type="ProfessionalLicenseType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ProfessionalLicenseType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="LicenseDetails" type="ProfessionalLicenseDetailsType" />
      <xsd:element minOccurs="0" name="LicenseDescription" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The license description</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssuingAuthority" type="ProfessionalLicenseIssuingAuthorityType" />
      <xsd:element minOccurs="0" name="ExpirationDate" type="DateType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The license expiration date</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="ProfessionalLicenseDetailsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The license name</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">250</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ProfessionalLicenseIssuingAuthorityType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The name of the issuing authority</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">250</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="JobCategoryType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="monsterId" type="xsd:string" use="required" />
        <xsd:attribute name="parentCategoryId" type="xsd:string" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="JobOccupationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>The name of this occupational category.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:long" use="required">
      <xsd:annotation>
        <xsd:documentation>The monster ID of this occupational category.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="parentCategoryId" type="xsd:long" use="optional">
      <xsd:annotation>
        <xsd:documentation>This field only has value for companies with custom categories. The ID of the monster category / occupational category that most closely matches this custom category. This field is for export purposes only.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="categoryId" type="xsd:long" use="optional">
      <xsd:annotation>
        <xsd:documentation>ID of the JobCategory the JobOccupation is tied to. This JobCategory has to be in the list of JobCategories. If only one JobCategory is present, this is optional.</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="Status" type="StatusType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Indicates if the transaction was succesful</definition>
        <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
        <datalength xmlns="http://schemas.monster.com/Monster">20</datalength>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="StatusType">
    <xsd:sequence>
      <xsd:element name="ReturnCode">
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attribute name="returnCodeType" use="required">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="success" />
                    <xsd:enumeration value="failure" />
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:attribute>
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
      <xsd:element minOccurs="0" ref="Descriptions" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:element name="Descriptions">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" maxOccurs="unbounded" ref="Description" />
        <xsd:element minOccurs="0" name="Errors" type="MessagesType" />
        <xsd:element minOccurs="0" name="Warnings" type="MessagesType" />
        <xsd:element minOccurs="0" name="Infos" type="MessagesType" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="Description">
    <xsd:complexType>
      <xsd:simpleContent>
        <xsd:extension base="xsd:string">
          <xsd:attribute name="descriptionType" use="required">
            <xsd:simpleType>
              <xsd:restriction base="xsd:string">
                <xsd:enumeration value="error" />
                <xsd:enumeration value="warning" />
                <xsd:enumeration value="info" />
              </xsd:restriction>
            </xsd:simpleType>
          </xsd:attribute>
        </xsd:extension>
      </xsd:simpleContent>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="MessagesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Message" type="MessageType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MessageType">
    <xsd:sequence>
      <xsd:element name="text" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="id" />
  </xsd:complexType>
  <xsd:complexType name="StringWithIdType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="monsterId" type="xsd:string" use="required" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="StringWithOptionalIdType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="monsterId" type="xsd:string" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="OccupationalClassificationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="occupationId" type="xsd:integer" />
    <xsd:attribute name="parentOccupationId" type="xsd:integer" />
  </xsd:complexType>
  <xsd:complexType name="OccupationalClassificationTier2Type">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="occupationId" type="MonsterOccupationIdTier2Enum" use="required" />
    <xsd:attribute name="parentOccupationId" type="xsd:integer" />
  </xsd:complexType>
  <xsd:element name="OccupationalClassification" type="OccupationalClassificationTier3Type">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">
					Identifies the lowest level occupation classification, based on the government's
					Standard Occupational Classification (SOC), for the job. 
					If one is not provided, Monster attempts to automatically choose one based on the job title. 
					The SOC is a 3 tier model. These classifications come from the lowest tier of the hiearchy.
				</definition>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="OccupationalClassificationTier3Type">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="occupationId" type="MonsterOccupationIdTier3Enum" use="required" />
    <xsd:attribute name="parentOccupationId" type="xsd:integer" />
  </xsd:complexType>
  <xsd:complexType name="LookupTableType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="MessageValue" type="StringWithOptionalIdType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Value stored in our database that is displayed for the user in the UI.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DescriptionMessage" type="StringWithOptionalIdType">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">8000</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="CreatedDate" />
      <xsd:element minOccurs="0" ref="ModifiedDate" />
    </xsd:sequence>
    <xsd:attribute name="lookupTableId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="SequencedLookupTableType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="LookupTableType">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="SortSequence" type="xsd:string" />
          <xsd:element minOccurs="0" name="IsFree" type="xsd:string" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:complexType name="CustomQuestionsType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="CustomQuestion" type="CustomQuestionType" />
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="CustomQuestionType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CQInternalPromptText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">For custom questions, this value is displayed for the user in the UI.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Message" type="StringWithIdType" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="CQAnswer">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">There are two types of questions - ones with a single Answer, and those with multiple Answers. If a question is multiple choice, customQuestionAnswerId is the value from the Answers table that the user selected.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="AnswerText" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">AnswerText is either the corresponding text from the CustomReferenceListItem for that customQuestionAnswerId or it's the AnswerText supplied by the user.</definition>
                  <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
                  <datalength xmlns="http://schemas.monster.com/Monster">125000</datalength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="PartnerAnswerRefCode" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">The Partner-specific value associated with the custom Answer.</definition>
                  <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
                  <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="CQAnswerAttributes">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">List of attributes qualifying the answer. This is currently only
									applicable to Custom Questions related to jobs.</definition>
                </xsd:documentation>
              </xsd:annotation>
              <xsd:complexType>
                <xsd:sequence>
                  <xsd:element maxOccurs="unbounded" name="CQAnswerAttribute" type="CQAnswerAttributeType" />
                </xsd:sequence>
              </xsd:complexType>
            </xsd:element>
          </xsd:sequence>
          <xsd:attribute name="customQuestionAnswerId" type="xsd:string" use="optional" />
          <xsd:attribute name="partnerAnswerId" type="xsd:string" use="optional" />
          <xsd:attribute name="monsterReference" type="xsd:string" use="optional" />
          <xsd:attribute name="monsterReferenceId" type="xsd:integer" use="optional" />
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="customQuestionId" type="xsd:string" />
    <xsd:attribute name="displaySequence" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="CQAnswerAttributeType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="AttributeText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Text that further qualifies the provided answer.
						While both are optional individually, AttributeText or the cqAnswerAttributeId must be provided.
						monsterId is for outbound purposes only.
						</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">2000</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          <definition xmlns="http://schemas.monster.com/Monster">Monster Internal: Maps to XXXCustomReferenceListItemSelectID. This field is outbound only.</definition>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
    <xsd:attribute name="cqAnswerAttributeId" type="xsd:string" use="optional">
      <xsd:annotation>
        <xsd:documentation>
          <definition xmlns="http://schemas.monster.com/Monster">Monster Internal: Maps to CustomReferenceListItemAttributeID.</definition>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="QuestionnaireType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Title" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Title of the Questionnaire</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">500</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="RecruiterReference" />
      <xsd:element minOccurs="0" name="HeaderText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Description of Questionnaire</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ScreenOutScore" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Minimum score of Questionnaire</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ScreenInLetter" type="StringWithIdType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Auto letter if applicant meets minimum requirements</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">6000</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ScreenOutLetter" type="StringWithIdType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Auto letter if applicant is screened out</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">6000</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IsFree" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Internal Monster value</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LastActivityDate" type="DateType" />
      <xsd:element minOccurs="0" ref="CreatedDate" />
      <xsd:element minOccurs="0" ref="ModifiedDate" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Question" type="QuestionType" />
    </xsd:sequence>
    <xsd:attribute name="questionnaireId" type="xsd:string" />
    <xsd:attribute name="questionnaireAction" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="QuestionType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DesiredResponse" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The desired answer to the question.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">500</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Operator" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"><![CDATA[Mathematical operator used for question (">","<","=")]]></definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">2</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="QuestionImportance" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Weight of this question</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="QuestionText" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The text of the question</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">300</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MultipleChoice" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Indicates if the question is mulitple choice or not</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IsFree" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Internal Monster value</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" ref="CreatedDate" />
      <xsd:element minOccurs="0" ref="ModifiedDate" />
      <xsd:element minOccurs="0" name="AnswerType" type="AnswerTypeEnumStr" />
      <xsd:element minOccurs="0" name="SortSequence" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The order in which the questions are presented to the user in the UI</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Answer">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="AnswerText" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">The text of the answer</definition>
                  <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
                  <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" name="SortSequence" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">The order in which the answers are presented to the user in the UI</definition>
                  <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
            <xsd:element minOccurs="0" ref="CreatedDate" />
            <xsd:element minOccurs="0" ref="ModifiedDate" />
          </xsd:sequence>
          <xsd:attribute name="answerId" type="xsd:string" />
          <xsd:attribute default="false" name="desired" type="xsd:boolean" use="optional" />
          <xsd:attribute default="false" name="isRequired" type="xsd:boolean" use="optional" />
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="questionId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DemographicDetailType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DateOfBirth" nillable="true" type="DateType" />
      <xsd:element minOccurs="0" name="Gender" nillable="true" type="GenderEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Gender of the JobSeeker</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Ethnicity" nillable="true" type="EthnicityEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Ethnicity of the JobSeeker</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SecurityClearance" nillable="true" type="SecurityClearanceType" />
      <xsd:element minOccurs="0" name="CurrentUSMilitaryInvolvement" type="USMilitaryInvolvementEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Details the type of US Military Involvement</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EUMilitaryStatus" type="EUMilitaryStatusEnum">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Details the type of EU Military Status</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EUMilitaryStatusEventDate" type="DateTimeType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The EU Military Status event date</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Disability" nillable="true" type="DisabilityType" />
      <xsd:element minOccurs="0" name="VictimOfTerrorism" nillable="true" type="VictimOfTerrorismType" />
      <xsd:element minOccurs="0" name="ExConvict" nillable="true" type="ExConvictType" />
      <xsd:element minOccurs="0" name="CitizenshipStatuses" nillable="true" type="CitizenshipStatusesType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SecurityClearanceType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ClearanceLevel" nillable="true" type="SecurityClearanceEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">1,"true" - indicate that person has secuirty clearance </definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssuingAuthority" nillable="true" type="SecurityClearanceIssuingAuthorityType" />
      <xsd:element minOccurs="0" name="InvestigationDate" nillable="true" type="DateType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SecurityClearanceIssuingAuthorityType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="SecurityClearanceIssuingAuthorityEnumStr" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CitizenshipStatusesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="CitizenshipStatus" type="CitizenshipStatusType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CitizenshipStatusType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Citizenship" type="LegalStatusesEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">JobSeeker's citizenship by country</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CountryCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Country code 2 letter abbrev for the country the Auth applies to</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">2</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CitizenshipIds" type="CitizenshipIdsType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CitizenshipIdsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CitizenshipId" type="CitizenshipIdType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CitizenshipIdType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Type" type="NationalIDTypeEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">citizen Id type</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Id" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">CitizenId</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ApplicantResponseType">
    <xsd:sequence>
      <xsd:element name="Question" type="QuestionType" />
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="QuestionResponse">
        <xsd:complexType>
          <xsd:simpleContent>
            <xsd:extension base="xsd:string">
              <xsd:attribute name="questionResponseType" use="required">
                <xsd:simpleType>
                  <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="Text" />
                    <xsd:enumeration value="Boolean" />
                    <xsd:enumeration value="Numeric" />
                    <xsd:enumeration value="AnswerId" />
                    <xsd:enumeration value="BooleanMaybe" />
                  </xsd:restriction>
                </xsd:simpleType>
              </xsd:attribute>
              <xsd:attribute name="questionResponseAnswerId" type="xsd:string" use="optional" />
            </xsd:extension>
          </xsd:simpleContent>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CurrencyType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">
				 1           USD
				 2           EUR
				 3           ARS
				 4           AUD
				 5           BEF
				 6           BRL
				 7           CAD
				 8           CHF
				 9           CNY
				 10          CZK
				 11          DEM
				 12          ESP
				 13          FJD
				 14          FRF
				 15          GBP
				 16          GRD
				 17          HKD
				 18          HUF
				 19          IDR
				 20          IEP
				 21          ILS
				 22          INR
				 23          ITL
				 24          JPY
				 25          KRW
				 26          MXN
				 27          MYR
				 28          NLG
				 29          NZD
				 30          PLZ
				 31          RUR
				 32          SEK
				 33          SGD
				 34          TWD
				 35          ZAR
				 36          LUF
				 37          NOK
				 38          DKK
				 39          FIM
				</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="monsterId" type="xsd:string" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="CreatedDate" type="DateTimeType" />
  <xsd:element name="LastActivityDate" type="DateTimeType" />
  <xsd:element name="ModifiedDate" type="DateTimeType" />
  <xsd:element name="UserOpenDate" type="DateTimeType" />
  <xsd:complexType name="DateRangeType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="StartDate" type="DateType" />
      <xsd:element minOccurs="0" name="EndDate" type="DateType" />
    </xsd:sequence>
    <xsd:attribute default="false" name="isCurrent" type="xsd:boolean" use="optional" />
  </xsd:complexType>
  <xsd:complexType name="DateTimeType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:dateTime" />
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:complexType name="DateType">
    <xsd:choice>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="Day" type="xsd:string" />
        <xsd:element minOccurs="0" name="Month" type="xsd:string" />
        <xsd:element minOccurs="0" name="Year" type="xsd:string" />
      </xsd:sequence>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="Date" type="xsd:string" />
      </xsd:sequence>
    </xsd:choice>
  </xsd:complexType>
  <xsd:simpleType name="MonthType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Month as 2 digit integer</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="1" />
      <xsd:maxInclusive value="12" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:simpleType name="YearType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Year as 4 digit integer</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:integer">
      <xsd:minInclusive value="1900" />
      <xsd:maxInclusive value="9999" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="UserReference" type="UserReferenceType" />
  <xsd:complexType name="UserReferenceType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="UserName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">20</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="PersonName" type="StructuredNameType">
        <xsd:annotation>
          <xsd:documentation>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">355</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="EmailAddress" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="userId" type="xsd:string" use="optional" />
  </xsd:complexType>
  <xsd:element name="RecruiterReference" type="RecruiterReferenceType" />
  <xsd:complexType name="RecruiterReferenceType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="UserReferenceType" />
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="PrivateApplicantReference" type="PrivateApplicantReferenceType" />
  <xsd:complexType name="PrivateApplicantReferenceType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="UserReferenceType">
        <xsd:sequence>
          <xsd:element name="CompanyReference" type="CompanyReferenceType" />
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="JobSeekerReference" type="JobSeekerReferenceType" />
  <xsd:complexType name="JobSeekerReferenceType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="UserReferenceType">
        <xsd:sequence>
          <xsd:element ref="Channel" />
        </xsd:sequence>
        <xsd:attribute name="partnerJobSeekerId" type="xsd:string" use="optional" />
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:element name="LetterReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="LetterTitle" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
              <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="letterId" type="xsd:int" />
      <xsd:attribute name="hiddenLetterId" type="xsd:int">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Updates of locked letters cause the original letter to be hidden. This is the letterId of the original letter.</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="BoardName" type="BoardGroupIdType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Board where this is active.</definition>
        <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
        <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
      </xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:complexType name="BoardGroupIdType">
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="monsterId" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              <definition xmlns="http://schemas.monster.com/Monster">The Monster-defined ID that identifies the board. See Monster for the appropriate values before submitting.</definition>
              <datatype xmlns="http://schemas.monster.com/Monster">integer</datatype>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
        <xsd:attribute name="boardGroupId" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              <definition xmlns="http://schemas.monster.com/Monster">The Monster-defined ID that identifies the board group. Specifying a boardGroupId allows postings to be "tied" together so that changes to one jobposting affect the associated "tied" jobpostings. See Monster for the appropriate values before submitting. If boardGroupId is submitted, you must submit the monsterId attribute as well.</definition>
              <datatype xmlns="http://schemas.monster.com/Monster">integer</datatype>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:attribute>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="Channel" type="ChannelType" />
  <xsd:complexType name="ChannelType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Internal Monster value</definition>
        <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
        <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="StringWithIdType">
        <xsd:attribute name="alias" type="xsd:string" use="optional" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:element name="QuestionnaireReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="QuestionnaireName" type="xsd:string" />
      </xsd:sequence>
      <xsd:attribute name="questionnaireId" type="xsd:string" use="optional" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="JobApplicantReference">
    <xsd:complexType>
      <xsd:attribute name="applicantId" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ResumeReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="ResumeTitle" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
              <datalength xmlns="http://schemas.monster.com/Monster">500</datalength>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="ResumeRefCode" type="xsd:string" />
      </xsd:sequence>
      <xsd:attribute name="textResumeId" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="FolderReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="FolderName" type="xsd:string" />
      </xsd:sequence>
      <xsd:attribute name="folderId" type="xsd:string" />
      <xsd:attribute name="folderRefCode" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="SubmissionReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="SubmissionName" type="xsd:string" />
      </xsd:sequence>
      <xsd:attribute name="submissionId" type="xsd:string" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="JobRequisitionReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="ReqTitle" type="xsd:string" />
      </xsd:sequence>
      <xsd:attribute name="jobRequisitionId" type="xsd:string" use="optional" />
      <xsd:attribute name="reqRefCode" type="xsd:string" use="optional" />
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="JobReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" name="JobTitle" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              <definition xmlns="http://schemas.monster.com/Monster">The title of the job.</definition>
              <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
              <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
        <xsd:element minOccurs="0" name="JobRankingScore" type="xsd:string">
          <xsd:annotation>
            <xsd:documentation>
              <definition xmlns="http://schemas.monster.com/Monster">The job ranking score</definition>
              <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
            </xsd:documentation>
          </xsd:annotation>
        </xsd:element>
      </xsd:sequence>
      <xsd:attribute name="jobRefCode" type="xsd:string" use="optional">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The recruiter-specific term used to uniquely identify a job.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="JobPostingReference">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element minOccurs="0" ref="JobReference" />
        <xsd:element minOccurs="0" ref="BoardName" />
      </xsd:sequence>
      <xsd:attribute name="postingId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Monster-specific ID used to uniquely identify a job. Also known as "JobID"</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
            <validation xmlns="http://schemas.monster.com/Monster">Not required; this will be generated by Monster.</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:attribute>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="CompanyReference" type="CompanyReferenceType" />
  <xsd:complexType name="CompanyReferenceType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CompanyXCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">A Monster unique identifier for a company.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">20</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CompanyName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The name of the company.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">255</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="companyId" type="xsd:string" />
  </xsd:complexType>
  <xsd:simpleType name="MoneyType">
    <xsd:restriction base="xsd:decimal">
      <xsd:fractionDigits value="4" />
      <xsd:minInclusive value="0.0000" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="DeliveryReceipt">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="ReceivedAt" type="xsd:string" />
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:element name="ProcessingReceipt">
    <xsd:complexType>
      <xsd:sequence>
        <xsd:element name="RequestDocElement" type="xsd:string" />
        <xsd:element name="Status" type="StatusType" />
        <xsd:element minOccurs="0" name="Response">
          <xsd:complexType>
            <xsd:sequence>
              <xsd:annotation>
                <xsd:documentation>Any response ok, lax means
								look for xsd but not an error if not found.</xsd:documentation>
              </xsd:annotation>
              <xsd:any minOccurs="0" maxOccurs="unbounded" namespace="##any" processContents="lax" />
            </xsd:sequence>
          </xsd:complexType>
        </xsd:element>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>
  <xsd:complexType name="WorkPreferencesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Weekend" type="WeekendEnumStr" />
      <xsd:element minOccurs="0" name="Overtime" type="xsd:boolean" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="WeekendEnumStr">
    <xsd:annotation>
      <xsd:documentation>
0	No
1	Yes
2	I prefer weekends
			</xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="xsd:string">
        <xsd:attribute name="monsterId" type="WeekendIdEnum" use="required" />
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
  <xsd:simpleType name="WeekendIdEnum">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="0" />
      <xsd:enumeration value="1" />
      <xsd:enumeration value="2" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="EducationFieldsOfStudyType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="EducationFieldOfStudy">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="FieldOfStudyName" type="xsd:string">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">string representing the field of study name</definition>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="EducationMajorsType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="EducationMajor">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element minOccurs="0" name="Major" type="AreaOfStudyEnumStr">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">Enumeration defining the name and Id of the major studied</definition>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="LanguagesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Language" type="LanguageType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="LanguageType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="LanguageName" type="LanguagesEnumString">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Languages Spoken from the Languages page</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LanguageProficiency" type="LanguageProficiencyEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Language Proficiency from the Languages page</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CareerHighlightsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="CareerHighlight" type="CareerHighlightType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CareerHighlightType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CareerHighlightDescription" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">description of career highlight</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="ExternalRequisitionsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="ExternalRequisition" type="ExternalRequisitionType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ExternalRequisitionType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="RequisitionReferenceCode" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The external Requisition code</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="InterestsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Interest" type="InterestType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="InterestType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="InterestName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The interest name</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="IndustryCertificationsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="IndustryCertification" type="IndustryCertificationType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="IndustryCertificationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="CertificationName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The name of the certification </definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="VendorName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The name of the vendor that provided the certification </definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MonthAcquired" type="MonthType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Month the certification was aquired [1-12]</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="YearAcquired" type="YearType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Year  the certification was aquired </definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="Summary" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The summary of the certification </definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ExpirationDate" type="DateType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The certification expiration date</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="AwardsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Award" type="AwardType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="AwardType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="AwardName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">TheAward name</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="AwardDescription" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Awards Description</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">string</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="IndustriesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Industry" type="IndustryType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="IndustryType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="IndustryName" type="MonsterIndustriesEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">An ID used to describe an Industry type.</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="CoverLetterType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="LetterTitle" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster"> For applicants : The title of the letter the user includes in the application - available inbound only; not a field that is exported. For JobSeekers : The title of the letter. It is exported outbound.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="LetterBody" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">For applicants : The body of the letter the user includes in the application. For JobSeekers : The body of the letter</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">6000</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="letterId" type="xsd:string" />
    <xsd:attribute name="action" use="optional">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="add" />
          <xsd:enumeration value="addOrReplace" />
          <xsd:enumeration value="update" />
          <xsd:enumeration value="updateOrClone" />
          <xsd:enumeration value="delete" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="LettersType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="Letter" type="CoverLetterType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SearchInformationType">
    <xsd:annotation>
      <xsd:documentation>
				This object contains data about a search that was made. It includes recruiter data for the person performing the search, data describing the search, and the date the search occured.
			</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element name="RecruiterReference" type="RecruiterReferenceType" />
      <xsd:element minOccurs="0" name="Keywords" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						All keywordsdata that qualified the search. This field has a maximum size of 4000 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element ref="Channel">
        <xsd:annotation>
          <xsd:documentation>
						The channel used to qualify the search. No channel value indicates channel data was not used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="SearchDate" type="DateTimeType">
        <xsd:annotation>
          <xsd:documentation>
						This field indicates the date and time the search occured.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="CreatedDate" type="DateTimeType">
        <xsd:annotation>
          <xsd:documentation>
						This field indicates the date and time the search was recorded in the DB.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" ref="BoardName">
        <xsd:annotation>
          <xsd:documentation />
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="QueryBehavior" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						Defines how keywords are used in a search. This field has a maximum size of 225 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RecentJobTitle" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The desired recent job title requested in the search if any. This field has a maximum size of 70 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TargetJobTitle" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The desired target job title requested in the search if any. This field has a maximum size of 70 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RecentOrTargetJobTitle" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The desired recent or target job title requested in the search if any. This field has a maximum size of 70 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RecentEmployeerCompanyName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The desired recent employeer copmany name requested in the search if any.  This field has a maximum size of 70 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RecentSchoolName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The desired recent school name requested in the search if any. This field has a maximum size of 25 characters.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="RecentJobDescription" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The desired recent job description. 
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TextResumeId" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            The desired recent textual resume identifier requested in the search if any.
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="JobStatus" type="JobStatusEnumStr">
        <xsd:annotation>
          <xsd:documentation>
						Each job status indicates a requested job status in the search if any.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="JobType" type="UserPreferredJobCodeEnumStr">
        <xsd:annotation>
          <xsd:documentation>
						Each job type indicates a requested job status in the search if any.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="EducationLevel" type="EducationDegreeEnumStr">
        <xsd:annotation>
          <xsd:documentation>
						The list of education levels requested during the search. No education levels indicate that no education level criteria was used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="CareerLevel" type="CareerLevelEnumStr">
        <xsd:annotation>
          <xsd:documentation>
						The list of career levels requested during the search. No career levels indicate that no career level criteria was used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="JobCategory" type="JobCategoryType">
        <xsd:annotation>
          <xsd:documentation>
						The list of company job categories requested during the search. No company job categories  indicate that no company job category criteria was used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ResidenceLocation" type="LocationType">
        <xsd:annotation>
          <xsd:documentation>
						The list of residence locations requested during the search. No residence locations indicate that no residence location criteria was used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ResidencePostalCodeRadius" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The postal code radius requested during the search. No values indicate that no residence postal code radius criteria was used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="TargetWorkLocation" type="LocationType">
        <xsd:annotation>
          <xsd:documentation>
						The list of target work locations requested during the search. No target work locations indicate that no target work location criteria was used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WillingToRelocate" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>
						A flag indicating the desired value requested in the search for ones willingness to relocate. no value indicates that this data was not used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="WorkAuthorizationCountries" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						A list of countries that of which the candidate should be authorized to work in one. No countries indicate that country criteria was not used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="TargetSalaryData" type="JobSalaryType">
        <xsd:annotation>
          <xsd:documentation>
						Target salary data desired in the search. No salary data indicates that salary criteria was not used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CurrentSecurityClearance" type="xsd:boolean">
        <xsd:annotation>
          <xsd:documentation>
						A flag indicatingwhether security clearance was required during this search. No value indicates that security clearance was not used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ModifiedDateMinimumAge" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						Used to qualify how recently a resume was edited for it to be returned.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ModifiedDateMaximumAge" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						Used to qualify how recently a resume was edited for it to be returned.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MaximumResultSet" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
						The maximum number of resumes to return.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="MonsterOccupationalClassification" type="OccupationalClassificationTier2Type">
        <xsd:annotation>
          <xsd:documentation>
						The Monster Occupational Classification (MOC) desired in the search. No MOC indicates that MOC criteria was not used to qualify the search.
					</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>
						Monster's internal unique identifier for the search.
					</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="SearchesInformationType">
    <xsd:sequence>
      <xsd:element maxOccurs="unbounded" name="SearchInformation" type="SearchInformationType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="NameAndAbbreviationType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Name" type="xsd:string" />
      <xsd:element minOccurs="0" name="Abbreviation" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="monsterId" type="xsd:string" use="required">
      <xsd:annotation>
        <xsd:documentation>
						Monster's internal unique identifier for the item.
					</xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="JobSalaryType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="Currency" type="CurrencyType">
        <xsd:annotation>
          <xsd:documentation>The currency of the salary</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SalaryMin" type="MoneyType">
        <xsd:annotation>
          <xsd:documentation>The minimum salary</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SalaryMax" type="MoneyType">
        <xsd:annotation>
          <xsd:documentation>The maximum salary</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="CompensationType" type="CompensationTypeEnumStr">
        <xsd:annotation>
          <xsd:documentation>The frequency of pay</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SalaryDescription" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>Description of the total compensation package provided by the company</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="SalaryRange" type="SalaryRangeType">
        <xsd:annotation>
          <xsd:documentation>Used for chief jobs only.</xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="SalaryRangeType">
    <xsd:annotation>
      <xsd:documentation>For Chief only. A lookup into
			a table of text salary ranges.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="SalaryMin" type="xsd:string" />
      <xsd:element minOccurs="0" name="SalaryMax" type="xsd:string" />
    </xsd:sequence>
    <xsd:attribute name="salaryRangeId" type="xsd:string" />
  </xsd:complexType>
  <xsd:complexType name="DocumentReferenceType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DocumentType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">DocumentType of the requested Document (DD-214, Transcript etc.,)</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">50</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="dd-214" />
            <xsd:enumeration value="of-306" />
            <xsd:enumeration value="sf-50" />
            <xsd:enumeration value="sf-15" />
            <xsd:enumeration value="transcript" />
            <xsd:enumeration value="other" />
            <xsd:enumeration value="cover_letter" />
            <xsd:enumeration value="secure_resume" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="FileType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">FileType of the requested Document (.doc, .pdf, .jpg etc.,)</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">10</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:string">
            <xsd:enumeration value="doc" />
            <xsd:enumeration value="pdf" />
            <xsd:enumeration value="jpg" />
            <xsd:enumeration value="jpeg" />
            <xsd:enumeration value="docx" />
            <xsd:enumeration value="gif" />
            <xsd:enumeration value="rtf" />
            <xsd:enumeration value="png" />
          </xsd:restriction>
        </xsd:simpleType>
      </xsd:element>
      <xsd:element minOccurs="0" name="DocumentName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">DocumentName of the requested Document sent for all document types</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="documentID" type="xsd:string">
      <xsd:annotation>
        <xsd:documentation>
          <definition xmlns="http://schemas.monster.com/Monster">Unique Identifier of the requested Document.  It is a string ID</definition>
          <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
          <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
          <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
        </xsd:documentation>
      </xsd:annotation>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="DocumentContentType">
    <xsd:complexContent mixed="false">
      <xsd:extension base="DocumentReferenceType">
        <xsd:sequence>
          <xsd:element minOccurs="0" name="DocumentContent" type="xsd:base64Binary">
            <xsd:annotation>
              <xsd:documentation>
                <definition xmlns="http://schemas.monster.com/Monster">Encoded Binary Content of the requested Document</definition>
                <datatype xmlns="http://schemas.monster.com/Monster">base64Binary</datatype>
                <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
              </xsd:documentation>
            </xsd:annotation>
          </xsd:element>
        </xsd:sequence>
      </xsd:extension>
    </xsd:complexContent>
  </xsd:complexType>
  <xsd:simpleType name="WorkingHoursPerWeekType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">This provides a value resticted to a max of 5 digits with a max of 2 digits after decimal point</definition>
        <datatype xmlns="http://schemas.monster.com/Monster">decimal</datatype>
        <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:decimal">
      <xsd:maxInclusive value="168" />
      <xsd:minInclusive value="0.00" />
      <xsd:fractionDigits value="2" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:complexType name="ResumeIconsType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="ResumeIcon" type="ResumeIconType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="ResumeIconType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="ResumeIconName" type="ResumeIconsEnumString">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The Resume Icon type</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Int</datatype>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UserDriversLicensesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="UserDriversLicense" type="UserDriversLicenseType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="UserDriversLicenseType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="UserDriversLicenseName" type="UserDriversLicenseEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The driver's license associated with the user</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="IssueDate" type="DateTimeType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">The date associated with the driver's license</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="DisabilityType">
    <xsd:sequence>
      <xsd:element minOccurs="0" name="DisabilityDescription" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">A description of the disability</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="DisabilityCategories">
        <xsd:complexType>
          <xsd:sequence>
            <xsd:element maxOccurs="unbounded" name="DisabilityCategory" type="DisabilityCategoryEnumStr">
              <xsd:annotation>
                <xsd:documentation>
                  <definition xmlns="http://schemas.monster.com/Monster">A disability category</definition>
                </xsd:documentation>
              </xsd:annotation>
            </xsd:element>
          </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="status" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="true" />
          <xsd:enumeration value="false" />
          <xsd:enumeration value="declinedToIdentify" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="VictimOfTerrorismType">
    <xsd:attribute name="status" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="true" />
          <xsd:enumeration value="false" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:complexType name="ExConvictType">
    <xsd:attribute name="status" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="true" />
          <xsd:enumeration value="false" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:simpleType name="RecruiterActionType">
    <xsd:restriction base="xsd:string">
      <xsd:enumeration value="add" />
      <xsd:enumeration value="addOrUpdate" />
      <xsd:enumeration value="update" />
      <xsd:enumeration value="ignore" />
    </xsd:restriction>
  </xsd:simpleType>
  <xsd:element name="Document" type="DocumentType" />
  <xsd:complexType name="DocumentType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Defines a document</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" name="File" type="xsd:base64Binary" />
      <xsd:element minOccurs="0" name="FileName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <definition xmlns="http://schemas.monster.com/Monster">Name of file, including file extension.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <datalength xmlns="http://schemas.monster.com/Monster">100</datalength>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="FileContentType" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation xml:lang="en">
            <definition xmlns="http://schemas.monster.com/Monster">Content/mime type associated with file.</definition>
            <datatype xmlns="http://schemas.monster.com/Monster">Text</datatype>
            <validation xmlns="http://schemas.monster.com/Monster">Required</validation>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
    <xsd:attribute name="documentAction" use="required">
      <xsd:simpleType>
        <xsd:restriction base="xsd:string">
          <xsd:enumeration value="add" />
          <xsd:enumeration value="deleteAndAdd" />
        </xsd:restriction>
      </xsd:simpleType>
    </xsd:attribute>
  </xsd:complexType>
  <xsd:element name="Documents" type="DocumentsType" />
  <xsd:complexType name="DocumentsType">
    <xsd:annotation>
      <xsd:documentation>
        <definition xmlns="http://schemas.monster.com/Monster">Defines a collection of documents</definition>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Document" type="DocumentType" />
    </xsd:sequence>
    <xsd:attribute name="resourceLicenseId" type="xsd:int" use="required" />
    <xsd:attribute name="boardId" type="xsd:int" use="required" />
    <xsd:attribute name="channelId" type="xsd:int" use="required" />
    <xsd:attribute name="languageCode" type="xsd:string" use="required" />
    <xsd:attribute name="countryCode" type="xsd:string" use="required" />
  </xsd:complexType>
  <xsd:complexType name="PropertiesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="Property" type="PropertyType">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Holder for all extension data related to a monster business object</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="PropertyType">
    <xsd:sequence>
      <xsd:element name="Name" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Name of property extension</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element name="Value" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">Value of property extension</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="OidClaimType">
    <xsd:sequence>
      <xsd:element name="OidProviderId" type="LookupOIDProvidersEnumStr">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">this is a provider id for the claim</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ClaimIdentifier" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">this is the OID Guid</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element minOccurs="0" name="ProviderGuid" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
            <definition xmlns="http://schemas.monster.com/Monster">this is the Provider OID Guid. Used for seamless login</definition>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MilitaryExperiencesType">
    <xsd:sequence>
      <xsd:element minOccurs="0" maxOccurs="unbounded" name="MilitaryExperience" type="MilitaryExperienceType" />
    </xsd:sequence>
  </xsd:complexType>
  <xsd:complexType name="MilitaryExperienceType">
    <xsd:sequence>
      <xsd:element name="UnitName" type="xsd:string">
        <xsd:annotation>
          <xsd:documentation>
